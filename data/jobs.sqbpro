="300"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table></browse_table_settings></tab_browse><tab_sql><sql name="Browse Jobs">SELECT
	Url,
	JobDescription,
	JobTitle,
	CompanyName,
	Location, 
	Remote, 
	Score, 
	IsDistributed,
	Notes,
	CASE 
		WHEN SalaryIsEstimated = 'true' THEN NULL
		ELSE
			CASE j.Currency
				WHEN 'PLN' THEN j.HourlyMax
				WHEN 'EUR' THEN j.HourlyMax * 4.22
				WHEN 'USD' THEN j.HourlyMax * 3.54
				WHEN 'GBP' THEN j.HourlyMax * 4.79
				ELSE NULL
			END
	END AS HourlyMaxPLN,

	SalaryOriginalText,
	DeliveryPressureScore,
	ContractType,
	Seniority,
	TechKeywords
FROM Jobs AS j
WHERE 
    j.JobTitle IS NOT NULL
    AND (j.Remote = 'remote' OR j.Location LIKE '%Wrocław%')
	AND CompanyName not in ('Sii Poland' , 'Luxoft')
	AND Rejected = 0
	AND Applied = 0
	And Marked = 0
ORDER BY j.Score DESC;



