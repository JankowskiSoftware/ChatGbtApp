</sql><sql name="Select*">SELECT
	datetime,
	Url,
	JobDescription,
	JobTitle,
	CompanyName,
	Location, 
	Remote, 
	PlRemote,
	Score, 
	IsDistributed,
	Notes,
	HourlyMaxPLN,
-- 	CASE 
-- 		WHEN SalaryIsEstimated = 'true' THEN NULL
-- 		ELSE
-- 			CASE j.Currency
-- 				WHEN 'PLN' THEN j.HourlyMax
-- 				WHEN 'EUR' THEN j.HourlyMax * 4.22
-- 				WHEN 'USD' THEN j.HourlyMax * 3.54
-- 				WHEN 'GBP' THEN j.HourlyMax * 4.79
-- 				ELSE NULL
-- 			END
-- 	END AS HourlyMaxPLN,

	SalaryOriginalText,
	DeliveryPressureScore,
	ContractType,
	Seniority,
	TechKeywords
FROM Jobs AS j
WHERE 
    j.JobTitle IS NOT NULL
    AND (j.Remote = 'remote' OR j.Location LIKE '%Wroc≈Çaw%')
	AND CompanyName not in ('Sii Poland' , 'Luxoft')
	AND Rejected = 0
	AND Applied = 0
	and Marked = 0
	and datetime &gt;= '2025-12-29'
ORDER BY j.Score DESC;
</sql><current_tab id="1"/></tab_sql></sqlb_project>
